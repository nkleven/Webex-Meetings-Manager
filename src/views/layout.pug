block variables
doctype html
html
  head
    meta(charset="utf-8")
    link(rel="shortcut icon", href="/img/favicon.ico")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="theme-color", content="#000000")
    title #{title} | Webex API Demo
    link(rel="stylesheet" href="/css/style.css")
    link(rel="stylesheet" href="/css/momentum-ui-icons.min.css")
    link(rel="stylesheet" href="/css/font-awesome.min.css")
    link(rel="stylesheet" href="/css/materialdesignicons.min.css")
    link(rel="stylesheet" href="/css/jquery-ui.min.css")
    script(src="/js/jquery.min.js")
    script(src="/js/jquery-ui.min.js") 
    script(src="/js/dev.js")
    block extra-head
  body
    div#root
      input#nodeEnv(name='nodeEnv', type='hidden' value=nodeEnv)
      input#betaSite(name='betaSite', type='hidden' value=betaSite)
      block layout-content